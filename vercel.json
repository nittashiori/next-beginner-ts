{
  "version": 2,
  "builds": [
    {
      "src": ".storybook/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/_next/webpack-hmr", "dest": "/_next/webpack-hmr" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/storybook-static/$1" }
  ]
}